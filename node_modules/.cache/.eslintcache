ss Data_Binding extends Component {\n  title = \"Vui\";\n  render() {\n    return (\n      <div className=\"container\">\n        <div className=\"card\" style={cardStyle}>\n          <img className=\"card-img-top\" src={imgSrc} alt=\"Card image cap\" />\n          <div className=\"card-body\">\n            <h5 className=\"card-title\">{this.title}</h5>\n            <p className=\"card-text\">\n              Some quick example text to build on the card title and make up the\n              bulk of the card's content.\n            </p>\n            <a href=\"#\" className=\"btn btn-primary\">\n              Go somewhere\n            </a>\n          </div>\n        </div>\n      </div>\n    );\n  }\n}\n","/Users/quang/Desktop/ReactJS/bc04_react/src/Conditional_Rendering/Conditional_Rendering.jsx",[],[],"/Users/quang/Desktop/ReactJS/bc04_react/src/Ex_Layout/Ex_Layout.jsx",[],[],"/Users/quang/Desktop/ReactJS/bc04_react/src/Event_Binding/Event_Binding.jsx",[],[],"/Users/quang/Desktop/ReactJS/bc04_react/src/Demo_State/Demo_State.jsx",[],[],"/Users/quang/Desktop/ReactJS/bc04_react/src/Ex_Layout/HeaderComponent.jsx",[],[],"/Users/quang/Desktop/ReactJS/bc04_react/src/Ex_Layout/ContentComponent.jsx",[],[],"/Users/quang/Desktop/ReactJS/bc04_react/src/Ex_Layout/NavigateComponent.jsx",[],[],"/Users/quang/Desktop/ReactJS/bc04_react/src/Ex_Layout/FooterComponent.jsx",[],[],"/Users/quang/Desktop/ReactJS/bc04_react/src/RenderWithMap/RenderWithMap.jsx",[],[],"/Users/quang/Desktop/ReactJS/bc04_react/src/Demo_Prop/Demo_Prop.jsx",["280"],[],"import React, { Component } from \"react\";\nimport UserInfo_Prop from \"./UserInfo_Prop\";\n\nexport default class Demo_Prop extends Component {\n  //set Object\n  state = {\n    username: \"Alice\",\n    age: 2,\n  };\n  //set function\n  //   handleChangeName = () => {\n  //     this.setState({ username: \"Bob\" });\n  //   };\n  handleChangeName = (name) => {\n    this.setState({ username: name });\n  };\n  render() {\n    return (\n      <div>\n        <p>Demo_Prop</p>\n        <UserInfo_Prop\n          username={this.state.username}\n          userage={this.state.age}\n          handleChangeName={this.handleChangeName}\n        />\n      </div>\n    );\n  }\n}\n","/Users/quang/Desktop/ReactJS/bc04_react/src/Demo_Prop/UserInfo_Prop.jsx",[],[],"/Users/quang/Desktop/ReactJS/bc04_react/src/RenderWithMap/SubComponent.jsx",["281","282","283"],[],"import React, { Component } from \"react\";\n\nexport default class extends Component {\n  render() {\n    return (\n      <div className=\"card container col-3\" style={{ width: \"18rem\" }}>\n        <img\n          className=\"card-img-top\"\n          src={this.props.data.img}\n          alt=\"Card image cap\"\n        />\n        <div className=\"card-body\">\n          <h5 className=\"card-title\">{this.props.data.name}</h5>\n          <p className=\"card-text\">\n            Some quick example text to build on the card title and make up the\n            bulk of the card's content.\n          </p>\n          <a href=\"#\" className=\"btn btn-primary\">\n            {this.props.data.price}\n          </a>\n        </div>\n      </div>\n    );\n  }\n}\n","/Users/quang/Desktop/ReactJS/bc04_react/src/Ex_ShoesShop/Ex_ShoesShop.jsx",["284","285","286","287","288","289","290","291"],[],"import React, { Component } from \"react\";\nimport CartShoes from \"./CartShoes\";\nimport { datashoesArr } from \"./Data_Shoes\";\nimport Detail_Shoes from \"./Detail_Shoes\";\nimport ListShoes from \"./ListShoes\";\n\nexport default class Ex_ShoesShop extends Component {\n  state = {\n    shoesArr: datashoesArr,\n    shoesdetail: datashoesArr[0],\n    cartShoes: [],\n  };\n  handleViewDetail = (idShoes) => {\n    let index = this.state.shoesArr.findIndex((item) => {\n      return item.id == idShoes.id;\n    });\n    index != -1 && this.setState({ shoesdetail: this.state.shoesArr[index] });\n  };\n  handleAddShoesCard = (shoes) => {\n    let cloneCartShoes = [...this.state.cartShoes];\n    let index = this.state.cartShoes.findIndex((item) => {\n      return item.id == shoes.id;\n    });\n    if (index == -1) {\n      //th sản phẩm chưa có trong giỏ hàng\n      let productCart = { ...shoes, amount: 1 };\n      cloneCartShoes.push(productCart);\n    } else {\n      //th sản phẩm đã có trong giỏ hàng\n      cloneCartShoes[index].amount++;\n    }\n    this.setState({ cartShoes: cloneCartShoes });\n  };\n  handleDeleteShoes = (shoes) => {\n    let cloneCartShoes = [...this.state.cartShoes];\n    let index = this.state.cartShoes.findIndex((item) => {\n      return item.id == shoes.id;\n    });\n    cloneCartShoes.splice(index, 1);\n    this.setState({ cartShoes: cloneCartShoes });\n  };\n  handleIncreaseAmount = (shoes) => {\n    let cloneCartShoes = [...this.state.cartShoes];\n    let index = this.state.cartShoes.findIndex((item) => {\n      return item.id == shoes.id;\n    });\n    cloneCartShoes[index].amount++;\n    console.log(\"cloneCartShoes[index]: \", cloneCartShoes[index]);\n    this.setState({ cartShoes: cloneCartShoes });\n  };\n  handleDecreaseAmount = (shoes) => {\n    let cloneCartShoes = [...this.state.cartShoes];\n    let index = this.state.cartShoes.findIndex((item) => {\n      return item.id == shoes.id;\n    });\n    if (cloneCartShoes[index].amount > 1) {\n      this.setState({ cartShoes: cloneCartShoes });\n      return cloneCartShoes[index].amount--;\n    } else {\n      cloneCartShoes.splice(index, 1);\n      this.setState({ cartShoes: cloneCartShoes });\n    }\n  };\n  render() {\n    return (\n      <div>\n        <CartShoes\n          cartShoes={this.state.cartShoes}\n          handleDeleteShoes={this.handleDeleteShoes}\n          handleDecreaseAmount={this.handleDecreaseAmount}\n          handleIncreaseAmount={this.handleIncreaseAmount}\n        />\n        <ListShoes\n          data={this.state.shoesArr}\n          handleViewDetail={this.handleViewDetail}\n          handleAddShoesCard={this.handleAddShoesCard}\n        />\n